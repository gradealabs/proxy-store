{
  "name": "@gradealabs/store-hocs",
  "version": "3.1.2",
  "description": "Stores as higher-order components",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "babel --plugins @babel/plugin-transform-runtime,@babel/plugin-transform-modules-commonjs --presets @babel/es2017 src --out-dir lib --ignore '**/*.test.js'",
    "prepare": "npm run build",
    "test": "mocha --require jsdom-setup.js --require @babel/register --require @babel/polyfill src/*.test.js"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/polyfill": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-es2017": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@babel/runtime": "^7.0.0-beta.49",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.10.0",
    "mocha": "^5.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sinon": "^5.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gradealabs/store-hocs.git"
  },
  "author": "Grade A Labs Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/gradealabs/store-hocs/issues"
  },
  "homepage": "https://github.com/gradealabs/store-hocs#readme",
  "dependencies": {
    "ejson": "^2.1.2"
  }
}
